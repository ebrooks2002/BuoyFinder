{
  "version": 8,
  "name": "World Coastline Style",
  "sources": {
    "local_mbtiles_source": {
      "type": "vector",
      "url": "{path_to_mbtiles}"
    }
  },
  "layers": [
    {
      "id": "background",
      "type": "background",
      "paint": {
        "background-color": "#ffffff"
      }
    },
    {
      "id": "coastline-layer",
      "type": "line",
      "source": "local_mbtiles_source",
      "source-layer": "coastline",
      "layout": {
        "line-join": "round",
        "line-cap": "round"
      },
      "paint": {
        "line-color": "#948325",
        "line-width": 1.25,
        "line-opacity": 0.4
      }
    }
  ]
}